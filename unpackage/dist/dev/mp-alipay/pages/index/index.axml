<view class="content"><image class="logo" src="{{$root.m0}}" ref="__r" data-ref="imgref"></image><view class="text-area"><text class="title">{{title}}</text><view>{{age}}</view><image src="{{img}}"></image></view><block a:for="{{arr}}" a:for-item="item" a:for-index="i" a:key="i"><view>{{"名字："+item.name+"---年龄："+item.age}}</view></block><block a:if="{{flag}}"><view>显示我</view></block><button data-event-opts="{{[['tap',[['tapHandle',[1,'$event']]]]]}}" onTap="__e">点我传参</button><button data-event-opts="{{[['tap',[['printTitle',['$event']]]]]}}" onTap="__e">点我不传参</button><button data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" catchTap="__e">点我修改title</button><button data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" onTap="__e">修改title</button><view id="target">target</view></view>