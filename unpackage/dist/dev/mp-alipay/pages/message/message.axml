<view>消息页面<button data-event-opts="{{[['tap',[['send',['$event']]]]]}}" onTap="__e">点我发送请求</button><block a:if="{{movieData}}"><view><text>{{"电影名字："+movieData.originalName}}</text><view>导演：<block a:for="{{movieData.director}}" a:for-item="item" a:for-index="__i0__"><text>{{''+item.data[0].name+''}}</text></block></view><view>{{"电影信息\n\t\t\t"+movieData.data[0].genre+"\n\t\t\t"+movieData.data[0].language+"\n\t\t\t"+movieData.data[0].description+"\n\t\t\t"+movieData.data[0].country+''}}</view></view></block><button type="primary" data-event-opts="{{[['tap',[['chooseImg',['$event']]]]]}}" onTap="__e">上传图片</button><view><block a:for="{{imgArr}}" a:for-item="item" a:for-index="__i1__" a:key="*this"><image src="{{item}}"></image></block></view></view>